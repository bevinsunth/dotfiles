# BODD Portal Management
alias bodd-sc='start_portal Consumer'
alias bodd-spc='stop-consumer'
alias bodd-sm='start-merchant'
alias bodd-spm='~/.config/zsh/bodd/bodd-portal.sh stop-merchant'
alias bodd-rc='~/.config/zsh/bodd/bodd-portal.sh restart-consumer'
alias bodd-rm='~/.config/zsh/bodd/bodd-portal.sh restart-merchant'
alias bodd-spa='~/.config/zsh/bodd/bodd-portal.sh stop-all'

#AWS
alias aws-login='aws sso login --profile bev-admin'
alias aws-login-int='aws sso login --profile integration-admin'
